---
interface Props {
    modal: string
}
const { modal } = Astro.props
---
<button type="button" class="py-2 px-3 inline-flex items-center gap-x-2 text-base rounded-lg font-semibold text-rose-500 ring-rose-600 ring-1 hover:ring-rose-500 hover:bg-rose-500 hover:text-white hover:bg-opacity-100 disabled:opacity-50 disabled:pointer-events-none transition-all ease-in-out duration-500" data-hs-overlay={modal}>
    Cancel
</button>